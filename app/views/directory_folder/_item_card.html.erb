<div class="col-2 m-2">
  <div class="card">
    <div class="card-header">
      <%= item.file_name %>
    </div>
    <img class="card-img-top" src="<%= rails_blob_url(item.image) %>">
    <div class="card-body">
      <blockquote class="blockquote mb-0">
        <p></p>
        <p><%= item.visibility %></p>
        <%= item.image.filename%>
       
      </blockquote>
    </div>
  </div>
</div>
